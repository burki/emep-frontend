<div role="tabpanel" class="tab-pane" id="statistics">
    {% import 'Shared/highcharts.html.twig' as highcharts %}
    {{ highcharts.head() }}

    <script type="text/javascript">
        $(document).ready(function() {
            $('#container-venues-types').highcharts({
                chart: {
                    plotBackgroundColor: null,
                        plotBorderWidth: null,
                        plotShadow: false,
                        type: 'pie'
                },
                title: {
                    text: ' '
                },
                tooltip: {
                    pointFormat: '{point.y} Organizing Bodies (<b>{point.percentage:.1f}%</b>)'
                },
                plotOptions: {
                    pie: {
                    }
                },
                series: [{
                    name: 'City:',
                    type: 'pie',
                    innerSize: '50%',
                    data: {{ indexDataNumberVenueType[0]|raw }}
                }]
            });
        });


        $(document).ready(function() {
            $('#container-countries').highcharts({
                chart: {
                    plotBackgroundColor: null,
                    plotBorderWidth: null,
                    plotShadow: false,
                    type: 'pie'
                },
                title: {
                    text: ' '
                },
                tooltip: {
                    pointFormat: '{point.y} Organizing Bodies (<b>{point.percentage:.1f}%</b>)'
                },
                plotOptions: {
                    pie: {
                    }
                },
                series: [{
                    name: 'City:',
                    type: 'pie',
                    innerSize: '50%',
                    data: {{ indexDataNumberCountries[0]|raw }}
                }]
            });
        });

    </script>


    <h3 class="chart-header" data-collapse="container-venues-types"><span class="chartsymbol">+</span>Types of Organizing Bodies <span class="charttypeTitle">(Pie Chart)</span></h3>
    <div class="collapse" id="container-venues-types" style="width: 100%; height: 400px; margin: 0 auto;"></div>

    <h3 class="chart-header" data-collapse="container-countries"><span class="chartsymbol">+</span>Countries of of Organizing Bodies <span class="charttypeTitle">(Pie Chart)</span></h3>
    <div class="collapse" id="container-countries" style="width: 100%; height: 400px; margin: 0 auto;"></div>


    <script src="{{ app.request.basepath }}/js/collapseChart.js"></script>
</div>